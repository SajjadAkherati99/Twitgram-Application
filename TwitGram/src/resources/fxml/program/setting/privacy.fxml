      <MenuButton styleClass="shownLabel" fx:id="privacyLabel" layoutX="440.0" layoutY="180.0" mnemonicParsing="false" text="Public">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#setPrivate" text="Private" />
          <MenuItem mnemonicParsing="false" onAction="#setPublic" text="Public" />
        </items>
      </MenuButton>
      <MenuButton styleClass="shownLabel" fx:id="lastSeenLabel" layoutX="440.0" layoutY="230.0" mnemonicParsing="false" text="everybody">

      <MenuButton styleClass="shownLabel" fx:id="activeLabel" layoutX="673.0" layoutY="466.0" mnemonicParsing="false" text="Active">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#activate" text="Active" />
          <MenuItem mnemonicParsing="false" onAction="#deactivate" text="deactivate" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>

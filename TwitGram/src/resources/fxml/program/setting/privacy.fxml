<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="577.0" prefWidth="800.0" stylesheets="@../../../styles/program/setting/privacy.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.view.program.setting.PrivacyAndSecurityController">
   <children>
      <Label styleClass="shownLabel" layoutX="180.0" layoutY="180.0" prefHeight="17.0" prefWidth="80.0" text="Privacy" />
